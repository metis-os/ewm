# battery notification
super + ctrl + t
        bash $HOME/.config/ewm/scripts/battery.sh

# date-time notification
super + ctrl + d
        bash $HOME/.config/ewm/scripts/date-time.sh

# brightness level notification
super + ctrl + b
        bash $HOME/.config/ewm/scripts/brightness.sh

# volume level notification
super + ctrl + v 
        bash $HOME/.config/ewm/scripts/volume.sh


